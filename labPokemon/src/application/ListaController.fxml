<BorderPane prefHeight="320.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ListaController">
   <center>
      <BorderPane prefHeight="249.0" prefWidth="293.0" BorderPane.alignment="CENTER">
         <center>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fitHeight="386.0" fitWidth="491.0" layoutX="-6.0">
                     <image>
                        <Image url="@../img/abrazo.png" />
                     </image>
                  </ImageView>
                  <VBox layoutX="14.0" layoutY="14.0" prefHeight="264.0" prefWidth="116.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#toShowListForName" prefHeight="48.0" prefWidth="127.0" text="Lista Nombre" />
                        <Button mnemonicParsing="false" onAction="#toShowForPoints" prefHeight="48.0" prefWidth="133.0" text="Lista Puntaje" />
                        <Button fx:id="buscarPokemon" mnemonicParsing="false" onAction="#mostrarNombrePokemonController" prefHeight="48.0" prefWidth="145.0" text="Buscar Pokemon" />
                        <Button fx:id="btnBuscarJugador" mnemonicParsing="false" onAction="#mostrarNombreJugadorController" prefHeight="59.0" prefWidth="131.0" text="Buscar Jugador" />
                        <Button fx:id="btnVolver" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="61.0" prefWidth="124.0" text="Volver" />
                     </children>
                  </VBox>
                  <ListView fx:id="listaNombre" layoutX="165.0" layoutY="30.0" prefHeight="327.0" prefWidth="117.0" />
                  <ListView fx:id="listaPuntos" layoutX="313.0" layoutY="31.0" prefHeight="331.0" prefWidth="119.0" />
                  <Label layoutX="157.0" layoutY="14.0" prefHeight="17.0" prefWidth="133.0" text="Lista Jugadores  Nombre" />
                  <Label layoutX="313.0" layoutY="14.0" prefHeight="17.0" prefWidth="133.0" text="Lista Jugadores Puntaje" />
               </children>
            </AnchorPane>
         </center>
      </BorderPane>
   </center>
</BorderPane>
