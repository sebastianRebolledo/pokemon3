<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ElegirController">
   <center>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <center>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="btnJugar" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="39.0" prefWidth="96.0" text="Jugar" />
                  <Button fx:id="btnCargarJuego" mnemonicParsing="false" onAction="#cargarJuegoDeMiComputadora" prefHeight="38.0" prefWidth="100.0" text="Cargar Juego" GridPane.columnIndex="1" />
               </children>
            </GridPane>
         </center>
      </BorderPane>
   </center>
</BorderPane>
